{
  "presets": [
    "@babel/preset-env",
    "@babel/preset-react"
  ],
  "plugins": [
    [
      "module-resolver", {
      "root": ["./src"],
      "alias": {
        "@hoc": "./src/hoc",
        "@services": "./src/services",
        "@components": "./src/components",
        "@helpers": "./src/helpers"
      }
    }
    ],
    "@babel/plugin-proposal-class-properties"
  ]
}
